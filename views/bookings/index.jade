                      div(style='position:relative')  
                td.fc-fri.fc-col5.fc-widget-content
                  div
                    .fc-day-content
                      div(style='position:relative')  
                td.fc-sat.fc-col6.fc-widget-content.fc-last
                  div
                    .fc-day-content
                      div(style='position:relative')  
                td.fc-agenda-gutter.fc-widget-content.fc-last(style='display: none;')  
          div(style= "position: absolute; z-index: 2; left: 0px; width: 100%; top: 17px;")
            .fc-agenda-divider.fc-widget-header
              .fc-agenda-divider-inner
            div(style='position: absolute; width: 100%; overflow-x: hidden; overflow-y: auto; height: 700px;')
              div(style='position:relative;width:100%;overflow:hidden')
                table.fc-agenda-slots(cellspacing= "0", style= "width:100%")
