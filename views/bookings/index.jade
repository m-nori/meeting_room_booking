extends ../layout

append css
  link(rel='stylesheet', href='/css/booking.css')
  link(rel='stylesheet', href='/css/calendar.css')
  link(rel='stylesheet', href='/css/calendar_custom.css')
  link(rel='stylesheet', href='/css/fullcalendar.css')

block content
  #content.container-fluid
    .row-fluid
      .span2
        h3#search-booking-title Search Booking
        .custom-calendar-wrap
          #custom-inner.custom-inner
            .custom-header.clearfix
              nav
                span#custom-prev.custom-prev
                span#custom-next.custom-next
              h4.custom-year
                span#custom-year
                span &nbsp;/&nbsp;
                span#custom-month
            #calendar.fc-calendar-container
      .span10
        a#new.btn.btn-primary(style= "margin-right: 20px;") New Booking
        a#back.btn &lt;
        a#next.btn &gt;
        hr
        .fc-view.fc-view-agendaWeek.fc-agenda(style= "position: relative;", unselectable= "on")
          table.fc-agenda-days.fc-border-separate(style='width: 100%; table-layout: fixed;', cellspacing='0')
            thead
              tr.fc-first.fc-last
                th.fc-agenda-axis.fc-widget-header.fc-first(style='width: 50px;')  
                th.fc-widget-name.fc-widget-header Sun 1/13
                th.fc-widget-name.fc-widget-header Mon 1/14
                th.fc-widget-name.fc-widget-header Tue 1/15
                th.fc-widget-name.fc-widget-header Wed 1/16
                th.fc-widget-name.fc-widget-header Thu 1/17
                th.fc-widget-name.fc-widget-header Fri 1/18
                th.fc-widget-name.fc-widget-header.fc-last Sat 1/19
                th.fc-agenda-gutter.fc-widget-header.fc-last(style='display: none;')  
            tbody
              tr.fc-first.fc-last
                th.fc-agenda-axis.fc-widget-header.fc-first  
                td.fc-sun.fc-col0.fc-widget-content
                  div(style='height: 700px;')
                    .fc-day-content
                      div(style='position:relative')  
                td.fc-mon.fc-col1.fc-widget-content
                  div
                    .fc-day-content
                      div(style='position:relative')  
                td.fc-tue.fc-col2.fc-widget-content
                  div
                    .fc-day-content
                      div(style='position:relative')  
                td.fc-wed.fc-col3.fc-widget-content
